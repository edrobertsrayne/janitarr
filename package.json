{
  "name": "janitarr",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch run src/index.ts",
    "test": "bun test tests/lib/ tests/services/ tests/storage/",
    "test:integration": "bun test tests/integration/",
    "test:ui": "cd ui && bunx vitest run",
    "test:e2e": "bunx playwright test",
    "test:all": "bun test tests/lib/ tests/services/ tests/storage/ && cd ui && bunx vitest run",
    "typecheck": "bunx tsc --noEmit",
    "lint": "bunx eslint ."
  },
  "devDependencies": {
    "@google/gemini-cli": "^0.24.0",
    "@playwright/test": "^1.49.0",
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "playwright": "^1.49.0",
    "tailwindcss": "3",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.2"
  }
}
